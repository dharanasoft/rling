<h1>Listing settings</h1>

<table>
  <tr>
    <th>Setting Name</th>
    <th>Descriptive Text</th>
    <th>Setting Type</th>
    <th>Setting Value</th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.name %></td>
    <td><%= setting.desc_text %></td>
    <td><%= setting.setting_type %></td>
    <td><%= setting.setting_value %></td>
    <td><%= link_to image_tag('edit.png',:class=>'image_link'), edit_setting_path(setting)%></td>
  </tr>

<% end %>
</table>

<br />
<%= link_to 'Back to Dashboard',admin_dashboard_path %>
