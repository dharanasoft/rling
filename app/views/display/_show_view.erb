<%  unless  @object_model.nil? %>

   <% if @object_model.model_submissions.empty? %>
      <%= " No Record for #{@object_model.name}"  %>
  
   <% else %>
      <% if page.view_type == "Grid" %>
      <%=  render :partial=>"show_grid_view",:locals=>{:page=>page} %>
      <% elsif page.view_type ==  "Table" %>
      <%=  render :partial=>"show_table_view",:locals=>{:page=>page} %>
      <%  end %>
  <% end %>

<%  else %>
  <%= "there is no such model!!!"  %>
<% end  %>
