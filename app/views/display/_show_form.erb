<div id="form_<%= page.id %>" class="page_form">
<% unless page.form_components.eql?([]) %>
    <%= form_tag "/display/create_submissions/#{page.id}", :multipart=>true do %>
      <% page.form_components.each do |form_component| %>
       <div class="form_component">
         <div class="form_component_heading form_component_heading_<%= form_component.component_name %>">
            <%= form_component.component_display_name %>:
         </div>
         <div class="mandatory_component"><%= form_component.mandatory ? "*" : "" %></div>
         <div class="form_component_value form_component_value_<%= form_component.component_name %>">
           <%= check_component_type(form_component,@form_submission,FormData) %>
         </div>
      </div>
      <% end %>
    <div class="form_submit"><%= submit_tag "Submit" %></div>
    <% end %>
  <% end %>
</div>
